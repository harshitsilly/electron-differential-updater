{
  "name": "EDU",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "env": "dev1",
  "copyright": "opyright © 2020 EDU",
  "scripts": {
    "start": "electron .",
    "dist": "npx electron-builder",
    "generateZipBlockMap": "node scripts/index.js"
  },
  "build": {
    "appId": "EDU",
    "compression": "store",
    "productName": "EDU",
    "copyright": "Copyright © 2020 EDU",
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:5500/dist",
        "channel": "latest",
        "useMultipleRangeRequest": false,
        "useAppSupportCache": false
      }
    ],
    "mac": {
      "target": [
        "zip"
      ],
      "asar": false,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist"
    },
    "win": {
      "target": "NSIS",
      "asar": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "deleteAppDataOnUninstall": true,
      "include": "build/scripts/uninstaller.nsh",
      "license": "EULA.txt",
      "warningsAsErrors": false
    },
    "files": [
      "**/*"
    ]
  },
  "devDependencies": {
    "electron": "^10.1.0",
    "electron-builder": "^22.8.0"
  },
  "dependencies": {
    "@imjs/electron-differential-updater": "5.0.2-1",
    "electron-log": "^4.2.4"
  }
}
